<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
        <beans>
      
        <bean id="e1" class="com.suchiit.pojo.Engine1">
        
        <property name="modelyear" value="2013">
        </property>
        
        </bean>
        
         <bean id="e2" class="com.suchiit.pojo.Engine1">
        
        <property name="modelyear" value="2014">
        
        
        </property>
        
        </bean>
         <bean id="e3" class="com.suchiit.pojo.Engine1">
        
        <property name="modelyear" value="2015">
        
        
        </property>
        
        </bean>
        
        
        
        
        
       
        <bean id="c" class="com.suchiit.pojo.Car1">
        <property name="carnames">
        <list>
        
        <value>Audi</value>
        <value>Honda</value>
        <value>Benz</value>
        
        </list>
        
        </property>
        
        <property name="engine">
        <list>
        <ref bean="e1"/>
        <ref bean="e2"/>
        <ref bean="e3"/>
        
        
        </list>
        
        </property>
        
        </bean>
        
        
        
        
        </beans>